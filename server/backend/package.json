{
  "name": "wap-backend",
  "version": "1.0.2",
  "description": "web anime player backend",
  "author": "kptcheesewhiz",
  "license": "ISC",
  "scripts": {
    "prebuild-frontend": "shx rm -rf ./src/public",
    "build-frontend": "cd ../frontend && npm run build",
    "postbuild-frontend": "shx mv ../frontend/out ./src/public && shx touch ./src/public/.gitkeep",
    "prebuild": "shx rm -rf ./out",
    "build": "nest build",
    "postbuild": "shx cp -R package* src/.env src/public out/",
    "predist": "shx rm -rf ./dist",
    "dist": "npm run build-frontend && npm run build && shx mv ./out ./dist",
    "postdist": "cd ./dist && npm --prod i",
    "dev": "nodemon --config nodemon.json",
    "start": "nest start --exec 'node -r ./tsconfig-paths.js -r ts-node/register'",
    "start:dev": "nest start --exec 'node -r ./tsconfig-paths.js -r ts-node/register' --watch",
    "start:debug": "nest start --exec 'node -r ./tsconfig-paths.js -r ts-node/register' --debug --watch",
    "start:prod": "node -r ./tsconfig-paths.js out/index",
    "start:docker": "node index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/serve-static": "^2.1.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.3.0",
    "ffprobe-static": "^3.0.0",
    "helmet": "^4.4.1",
    "http-proxy-middleware": "^1.1.1",
    "jsdom": "^16.5.1",
    "mime-types": "^2.1.30",
    "node-fetch": "^2.6.1",
    "parse-torrent": "^9.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.6",
    "uuid": "^8.3.2",
    "webtorrent": "^0.115.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/helmet": "^4.0.0",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/jsdom": "^16.2.7",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@types/parse-torrent": "^5.8.3",
    "@types/uuid": "^8.3.0",
    "@types/webtorrent": "^0.109.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "shx": "^0.3.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
